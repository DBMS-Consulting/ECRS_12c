<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="2.2" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
  </application>
  <managed-bean eager="true">
    <managed-bean-name>crsBean</managed-bean-name>
    <managed-bean-class>com.novartis.ecrs.view.beans.CRSBean</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>
  <converter>
    <converter-id>TrimEndSpaces</converter-id>
    <converter-class>com.novartis.ecrs.view.converter.TrimEndSpacesConverter</converter-class>
  </converter>
  <converter>
    <converter-id>ConvertFirstLetterToCapital</converter-id>
    <converter-class>com.novartis.ecrs.view.converter.ChangeFirstLetterToCapital</converter-class>
  </converter>
</faces-config>